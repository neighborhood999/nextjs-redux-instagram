{
  "name": "nextjs-redux-instagram",
  "version": "1.0.0",
  "description": "Building Instagram like by next.js & redux.",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "test": "npm run flow && jest --updateSnapshot && jest --verbose",
    "test:watch": "jest --watch",
    "test:specify": "jest -t",
    "test:cov": "jest --coverage",
    "flow": "flow"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/components/__tests__/setup.js"
    ],
    "testMatch": [
      "<rootDir>/*/__tests__/*.spec.js"
    ]
  },
  "repository": "git@github.com:neighborhood999/nextjs-redux-instagram.git",
  "author": "Peng Jie <bivinity.pengzjie@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "dotenv": "^4.0.0",
    "fetch-jsonp": "^1.1.3",
    "next": "^3.2.1",
    "next-redux-wrapper": "^1.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-relative-time": "^0.0.7",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "semantic-ui-react": "^0.74.2",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "flow-bin": "^0.56.0",
    "jest": "^21.2.1",
    "object-assign-deep": "^0.3.1",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6"
  }
}
